{
  "name": "plataforma-anime",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
  "scripts": {
    "dev": "vite",
    "unlighthouse": "npx unlighthouse",
    "dev-unlighthouse": "concurrently \"pnpm run dev\" \"wait-on http://localhost:5173 && npm run unlighthouse\"",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:all": "pnpm test && pnpm test:e2e",
    "commit": "powershell -ExecutionPolicy Bypass -File ./auto-commit.ps1",
    "quick-commit": "git add . && git commit -m 'feat: quick update' && git push",
    "save": "npm run test && npm run commit",
    "commit-helper": "node commit-helper.js",
    "setup-git": "git config commit.template .gitmessage"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.42.1",
    "@clerk/themes": "^2.4.10",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-query-devtools": "^5.85.3",
    "@tanstack/react-router": "^1.131.14",
    "axios": "^1.11.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.539.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-toastify": "^11.0.5",
    "swiper": "^11.2.10",
    "tailwindcss": "^4.1.12",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@unlighthouse/vite": "^0.10.6",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.0",
    "cypress": "^13.6.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "msw": "^2.10.5",
    "vite": "^7.1.2",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.4"
  }
}
